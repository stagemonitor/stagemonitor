{
	"title": "Request",
	"services": {
		"filter": {
			"list": [
				{
					"type": "filter",
					"name": "application",
					"query": "stagemonitor.*",
					"includeAll": false
				},
				{
					"type": "filter",
					"name": "instance",
					"query": "stagemonitor.[[application]].*",
					"includeAll": true
				},
				{
					"type": "filter",
					"name": "host",
					"query": "stagemonitor.[[application]].[[instance]].*",
					"includeAll": true
				},
				{
					"type": "filter",
					"name": "request",
					"query": "stagemonitor.[[application]].[[instance]].[[host]].request.total.*",
					"includeAll": true
				}
			],
			"time": {
				"from": "now-6h",
				"to": "now"
			}
		}
	},
